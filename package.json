{
  "name": "min",
  "version": "13.0.0",
  "description": "Collection of functions used for DOM manipulations in Userscripts",
  "main": "min.js",
  "scripts": {
    "lint-code": "eslint min.js",
    "build-code": "uglifyjs min.js --compress --mangle --output min.min.js",
    "lint-docs": "documentation lint",
    "build-docs": "documentation readme --section 'Documentation' --quiet",
    "build": "npm run lint-code && npm run build-code && npm run lint-docs && npm run build-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revolter/min.git"
  },
  "keywords": [
    "userscript"
  ],
  "author": "Iulian Onofrei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revolter/min/issues"
  },
  "homepage": "https://github.com/revolter/min#readme",
  "devDependencies": {
    "documentation": "^6.3.3",
    "eslint": "^5.16.0",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
